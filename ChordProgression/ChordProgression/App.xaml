<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ChordProgression.App">
	<Application.Resources>

        <ResourceDictionary>
            <OnPlatform x:TypeArguments="x:String" x:Key="MuseJazzFont">
                <On Platform="Android" Value="MuseJazz.ttf#MuseJazz" />
                <On Platform="UWP" Value="/Assets/MuseJazz.ttf#MuseJazz" />
                <On Platform="iOS" Value="MuseJazz" />
            </OnPlatform>
        </ResourceDictionary>

	</Application.Resources>
</Application>